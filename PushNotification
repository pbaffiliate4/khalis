
var notificationStr = "{\"aps\": {\"alert\":\"batata ya baba ...\",\"sound\":\"default\"}}";

var store = "PushNotificationsStore";
var channelID = request.parameters["channelId"];
var applicationID = request.parameters["applicationId"];
var fireAndForgetValue = "false";


var paramsObj = {
		"apsdb.store": store,
		"channelId": channelID,
		"fireAndForget": fireAndForgetValue, 
		"notification": notificationStr
};               

var pushNotificationsResult = apsdb.callApi("PushNotification", paramsObj, null);

if (pushNotificationsResult.metadata.status == "failure") {
       return { status: "failure", errorCode: pushNotificationsResult.metadata.errorCode, errorDetail: pushNotificationsResult.metadata.errorDetail };
}

return pushNotificationsResult;